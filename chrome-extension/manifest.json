{
	"manifest_version": 3,
	"name": "Keyword Business Finder",
	"version": "1.0",
	"description": "Enter a business keyword, city, and state to search.",
	"permissions": ["storage", "activeTab"],
	"action": {
		"default_popup": "popup.html",
		"default_icon": {
			"16": "icon16.png",
			"48": "icon48.png",
			"128": "icon128.png"
		}
	}
}
